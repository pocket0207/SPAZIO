<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>JoinHandler</servlet-name>
        <servlet-class>newpackage.JoinHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckDuplicate</servlet-name>
        <servlet-class>newpackage.CheckDuplicate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginHandler</servlet-name>
        <servlet-class>newpackage.LoginHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FindIDHandler</servlet-name>
        <servlet-class>newpackage.FindIDHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FindPWDHandler</servlet-name>
        <servlet-class>newpackage.FindPWDHandler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePWDHandler</servlet-name>
        <servlet-class>newpackage.ChangePWDHandler</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JoinHandler</servlet-name>
        <url-pattern>/JoinHandler.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckDuplicate</servlet-name>
        <url-pattern>/CheckDuplicate.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginHandler</servlet-name>
        <url-pattern>/LoginHandler.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FindIDHandler</servlet-name>
        <url-pattern>/FindIDHandler.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FindPWDHandler</servlet-name>
        <url-pattern>/FindPWDHandler.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePWDHandler</servlet-name>
        <url-pattern>/ChangePWDHandler.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
